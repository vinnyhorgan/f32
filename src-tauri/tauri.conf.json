{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "f32",
  "version": "0.1.0",
  "identifier": "dev.vinnyhorgan.f32",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "flux32",
        "width": 960,
        "height": 540,
        "minWidth": 960,
        "minHeight": 540,
        "resizable": true,
        "visible": false,
        "backgroundColor": "#16161e",
        "transparent": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
